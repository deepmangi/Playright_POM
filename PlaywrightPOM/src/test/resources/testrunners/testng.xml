<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite thread-count="3" parallel="tests" name="Suite">
  
	<listeners>
		<listener
			class-name="listeners.ExtentReportListener" />
		</listeners>

	<test name="Open Cart Home Page Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="tests.HomePageTest" />
		</classes>
	</test>
	
	<test name="Open Cart Home Page Test_Edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="tests.HomePageTest" />
		</classes>
	</test>
	
	<test name="Open Cart Home Page Test_firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="tests.HomePageTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
